<html>
  <head>
    <link rel="preconnect" href="https://fonts.gstatic.com" />
    <link
      href="https://fonts.googleapis.com/css2?family=Fredoka+One&family=Oi&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl"
      crossorigin="anonymous"
    />
    <style>
      * {
        margin: 0;
        padding: 0;
      }
      body {
        width: 100%;
        height: calc(100vh - 80px);
        background: linear-gradient(
          90deg,
          rgba(255, 0, 233, 1) 0%,
          rgba(255, 153, 0, 1) 100%
        );
        /* background-position:center bottom;
      background-attachment: fixed; */
        overflow-x: hidden;
      }
      .heading {
        /* display: flex; */
        flex: 0.2;
        text-align: left;
        padding-left: 10px;

        /* flex-direction: column; */
        /* align-items: center; */
      }
      .heading1 {
        /* display: flex; */
        /* flex-direction: column; */
        text-align: center;
        flex: 0.8;
      }
      /* .brand {
        display: flex;
        margin-top: 5px;
        float: left;
      } */
      #siyor {
        font-family: "Fredoka One", cursive;
        color: #3c91fc;
        font-size: 40px;
      }

      #head {
        font-family: "Fredoka One", cursive;
        color: whitesmoke;
        font-size: 70px;
      }
      #head2 {
        font-family: "Fredoka One", cursive;
        color: whitesmoke;
        font-size: 80px;
      }

      .background {
        width: 100%;
        position: absolute;
        height: calc(100vh - 242px);
      }

      .char {
        width: 16%;
        z-index: 99999;
        position: relative;
        right: -10em;
        bottom: -12em;
      }

      .cab {
        width: 16%;
        z-index: 99999;
        position: relative;
        right: -40em;
        bottom: -18em;
      }
      .light {
        width: 8%;
        z-index: 99999;
        position: relative;
        right: -36em;
        bottom: -12em;
      }
      .dot {
        height: 15px;
        width: 15px;
        background-color: white;
        border-radius: 50%;
        display: inline-block;
      }
      .dot1 {
        height: 15px;
        width: 15px;
        background-color: black;
        border-radius: 50%;
        display: inline-block;
      }
      .bottomdot {
        text-align: center;
        position: fixed;
        bottom: 20;
        right: 45%;
        /*isko display karo*/
      }
      .betspace {
        width: 25px;
        height: 25px;
        display: inline-block;
      }
      #btndisclaimer {
        position: fixed;
        right: 20;
        bottom: 20;
        border-radius: 20px;
        /*isko display karo*/
      }
      /*page2 CSS*/
      .secondpage {
        display: none;
      }
      #title_top {
        color: #3c91fc;
        font-size: 40px;
        font-family: "Fredoka One", cursive;
      }
      #head {
        font-family: "Fredoka One", cursive;
        color: whitesmoke;
        font-size: 70px;
        margin-top: 9px;
      }
      #head2 {
        font-family: "Fredoka One", cursive;
        color: whitesmoke;
        font-size: 50px;
      }

      #head3 {
        font-family: "Fredoka One", cursive;
        color: whitesmoke;
        font-size: 50px;
      }

      .wrapper {
        background: linear-gradient(
          90deg,
          rgba(255, 0, 233, 1) 0%,
          rgba(255, 153, 0, 1) 100%
        );
      }

      .mobile {
        width: 35%;
        margin-top: 2%;
      }

      @media only screen and (max-width: 600px) {
        
      }
    </style>
  </head>
  <body>
    <div class="firstpage">
      <div class="heading">
        <h1 id="siyor">SIYOR</h1>
      </div>

      <div class="heading1">
        <h1 id="head">Welcome To</h1>
        <p id="head2">SIYOR CARE</p>
      </div>
      <img src="/image/BG.png" class="background" />
      <img src="/image/Character.png" class="char" />
      <img src="/image/Cab.png" class="cab" />
      <img src="image/Light Lamp.png" class="light" />
      <div class="bottomdot">
        <span class="dot1"></span>
        <span class="betspace"></span>
        <span class="dot"></span>
        <span class="betspace"></span>
        <span class="dot"></span>
      </div>

      <button
        type="submit"
        class="btn btn-light btn-md"
        id="btndisclaimer"
        onclick="page2()"
      >
        <span> Next </span>
      </button>
    </div>
    <div class="secondpage">
      <div>
        <h1 id="title_top">SIYOR</h1>
        <center>
          <h5 id="head">Send Conveninent and affordable ride</h5>
          <!-- <h6 id="head2">to the people who matters to your organization</h6> -->
          <p id="head2">to the people who matters to your organization</p>
          <p id="head3">No Smartphone Required</p>
          <img src="image/Mobile Buildings.png" class="mobile" />
        </center>
      </div>

      <div class="bottomdot">
        <span class="dot"></span>
        <span class="betspace"></span>
        <span class="dot1"></span>
        <span class="betspace"></span>
        <span class="dot"></span>
      </div>

      <a href="/login-agent" class="btn btn-light btn-md" id="btndisclaimer">
        Next
      </a>
    </div>
    <script>
      function page2() {
        document.querySelector(".firstpage").style.display = "none";
        document.querySelector(".secondpage").style.display = "block";
      }
    </script>
  </body>
</html>
